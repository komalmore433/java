import java.util.*;
class P3
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		String str=sc.next();
		String str1=str.trim();
		int len=0;
		for(int i=str1.length()-1;i>0;i--)
		{
			if(str1.charAt(i)== ' ' )
			{
				len=0;
			}
			else 
			{
				len++;
			}
		}
		System.out.println(len);
	}
	
}
